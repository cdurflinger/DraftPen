
{{> sessionHeader}}

<h1>Welcome admin!</h1>
<br>
<br>
<br>
<div id="userDashboardContainer">
    <h2>All User Information</h2>
    {{#each userData}}
        <div id={{username}} class="userDataContainer">
            <p>Username: {{username}}</p>
            <p>Permission Level: {{permissionLevel}}</p>
            <p>Email: {{email}}</p>
            <p>First Name: {{firstName}}</p>
            <p>Last Name: {{lastName}}</p>
            <a class="userProfileLinks">Click here to modify user/users blog posts</a>
        </div>
        <hr>
    {{/each}}
</div>

{{> sessionFooter}}